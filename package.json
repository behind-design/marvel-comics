{
  "name": "marvel-comics",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "lodash.chunk": "^4.2.0",
    "lodash.uniq": "^4.5.0",
    "normalizr": "^3.2.4",
    "query-string": "^5.0.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-loading": "^1.0.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts": "^1.0.17",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.17.0",
    "rxjs": "^5.5.2",
    "truncatise": "^0.0.6"
  },
  "scripts": {
    "build-css": "node-sass sass --output src/css --source-map --importer node_modules/compass-importer",
    "start-css": "node-sass-chokidar sass -o src/css --watch --recursive --importer node_modules/compass-importer",
    "build-js": "react-scripts build",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p start-css start-js",
    "build": "npm-run-all -p build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "compass-importer": "^0.4.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "jest-enzyme": "^4.0.1",
    "node-sass": "^4.7.1",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.1.1"
